{
    "name": "Stick It",
    "version": "0.1",
    "description": "Add sticky notes directly to webpages.",
    "manifest_version": 2,
    "browser_action": {
      "default_title": "Stick It!"
    },
    "background": {
      "scripts": [
        "background.js",
        "vendor.js",
        "runtime.js"
      ]
    },
    "permissions": [
      "https://*/*",
      "http://*/*",
      "webNavigation"
    ],
    "web_accessible_resources": [
      "index.html"
    ]
}
